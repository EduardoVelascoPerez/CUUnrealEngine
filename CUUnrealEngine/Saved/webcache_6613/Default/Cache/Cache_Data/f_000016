          if (continue_button.disabled) {
            return;
          }
          var enable = function(continue_button) {
            continue_button.disabled = false;
          };
          setTimeout(enable, 60000, continue_button);
          continue_button.disabled = true;
        }

          var _request = new XMLHttpRequest();
